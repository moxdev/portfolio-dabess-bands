#main {
  text-align: center;
  width: 960px;
  margin: 0 auto;
}

ul {
  list-style-type: none;
  margin: 0;
  padding: 0;
}

img {
  max-width: 150px;
}

form {
  width: 600px;
  margin: 0 auto;
}
form > * {
  display: block;
}

form input, form textarea {
  width: 100%;
  margin-bottom: 1em;
  padding: 10px;
}